                "EXPO_PUBLIC_GOOGLE_CLIENT_ID_WEB": "240781979317-th80om2srfbroe5kv9e6tfd86tglroqc.apps.googleusercontent.com",
                "EXPO_PUBLIC_GOOGLE_CLIENT_ID_ANDROID": "240781979317-bjl813ouj9hma6vnheba5l0uhcka9t9u.apps.googleusercontent.com"
            },
            "ios": {
                "resourceClass": "m1-medium"
            },
            "android": {
                "resourceClass": "large"
            }
        }
    },
    "submit": {
        "production": {
            "ios": {
