{
    "cli": {
        "version": ">= 3.0.0",
        "appVersionSource": "local"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "m1-medium"
            }
        },
        "development-simulator": {
            "developmentClient": true,
            "distribution": "internal",
