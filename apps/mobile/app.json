{
  "expo": {
    "name": "Sift",
    "slug": "sift",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sift",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#FDFCF8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hkjstudio.sift",
      "buildNumber": "11",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "sift"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FDFCF8"
      },
      "package": "com.hkjstudio.sift",
      "versionCode": 2,
      "allowBackup": false,
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "sift"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        },
        {
          "action": "SEND",
          "category": [
            "DEFAULT"
          ],
          "data": [
            {
              "mimeType": "text/plain"
            }
          ]
        }
      ],
      "permissions": [
        "INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-share-intent",
        {
          "iosAppGroupIdentifier": "group.com.hkjstudio.sift",
          "iosActivationRules": {
            "NSExtensionActivationSupportsWebURLWithMaxCount": 1
          },
          "android": {
            "disableCreateTask": false
          },
          "appleTeamId": "8DA3BAGA32"
        }
      ],
      "./plugins/withShareExtensionExclusions.js"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f97e75c-3da1-403c-9e5a-92156c51f400"
      }
    },
    "owner": "hkjstudio"
  }
}